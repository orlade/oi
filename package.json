{
  "name": "@oi/oi",
  "version": "0.1.0",
  "description": "CLI and core components of Oi",
  "author": "Oliver Lade <piemaster21@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/orlade/oi.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "bin": {
    "oi": "dist/bin/oi.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && node_modules/.bin/babel src --out-dir dist && echo 'Build complete'",
    "upgrade": "npm run build && oi update",
    "prepublish": "npm run build",
    "test": "mocha --recursive -R spec --compilers js:babel-core/register",
    "watch": "node_modules/.bin/watch 'npm run build && npm test' ./src ./test"
  },
  "dependencies": {
    "colors": "^1.1.2",
    "expand-home-dir": "0.0.3",
    "js-plugins": "0.0.3",
    "lodash": "^4.17.2",
    "moment": "2.17.1",
    "require-dir": "^0.3.0",
    "shelljs": "^0.7.5",
    "touch": "^1.0.0",
    "universal-mixin": "^2.0.0",
    "winston": "^2.3.0",
    "yargs": "^6.5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-env": "^1.1.4",
    "chai": "^3.5.0",
    "lodash": "^4.17.2",
    "mocha": "^3.2.0",
    "watch": "latest"
  }
}
